single val NativeMath =: {
	val nativePow = [Double base, Double exponent] => Double : {
		return pow(base, exponent);
	}

	val nativeLog = [Double value] => Double : {
		return log(value);
	}

	val nativeFloor = [Double value] => Double : {
		return floor(value);
	}

	val nativeCeil = [Double value] => Double : {
		return ceil(value);
	}
}