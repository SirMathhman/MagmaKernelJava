single val StringBinding =: {
	val length = [String value] => Int : {
		return strlen(value);
	}

	val copy = [String dest, String src] : {
		strcpy(dest, src);
	}

	val concat = [String dest, String src] : {
		strcat(dest, src);
	}

	val compare = [String value0, String value1] => Int : {
		return strcmp(value0, value1);
	}
}
